{
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "common": "readonly",
    "application": "readonly",
    "config": "readonly",
    "db": "readonly",
    "lib": "readonly",
    "domain": "readonly"
  },
  "rules": {
    "strict": [
      "error",
      "never"
    ],
    "id-denylist": [
      2,
      "common",
      "console",
      "application",
      "config",
      "db",
      "lib",
      "domain",
      "api"
    ]
  }
}
