{
  "name": "metatech-university-nodejs2023-application-prisma",
  "version": "1.0.0",
  "description": "Application domain layer with models defined in Prisma format. Metatech University educational project by Vladyslav Karpenko (KLarpen). Course NodeJS 2022-2023: how to use DDD, SOLID, GRASP and GoF in JavaScript and Node.js",
  "license": "MIT",
  "private": true,
  "main": "main.js",
  "scripts": {
    "test": "npm run lint && tsc",
    "lint": "eslint . && prettier -c \"**/*.js\" \"**/*.ts\"",
    "fmt": "prettier --write \"**/*.js\" \"**/*.ts\""
  },
  "prisma": {
    "seed": "node db/seed.js"
  },
  "engines": {
    "node": "16 || 18 || 20"
  },
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "pg": "^8.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@types/pg": "^8.10.2",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0",
    "prisma-json-schema-generator": "^3.1.4",
    "typescript": "^5.1.3"
  }
}
